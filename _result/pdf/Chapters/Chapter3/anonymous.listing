|person anonClass|
  person := Person new.
	anonClass := anObject class newAnonymousSubclass.
	anonClass
		compile:
			'name: aName
				self tag: aName.
				name := aName'.
  "migrates the object to its new class"
	anonClass adoptInstance: person.
  "migrates back the object to its original class"
  anonClass superclass adoptInstance: person.
