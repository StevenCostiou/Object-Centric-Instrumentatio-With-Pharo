|person anonClass|
  person := Person new.
	anonClass := anObject class newAnonymousSubclass.
	anonClass
		compile:
			'name: aName
				self tag: aName.
				name := aName'.
	anonClass adoptInstance: person. "migrates the object to its new class"
  anonClass superclass adoptInstance: "migrates back the object to its original class"
	^anonClass
